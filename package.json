{
    "name": "exiv2-wasm",
    "version": "0.5.12",
    "description": "Exiv2 in the browser via WebAssembly (read/write EXIF/IPTC/XMP).",
    "license": "MIT",
    "author": "tremolo5",
    "homepage": "https://github.com/gerosyab/exiv2-wasm",
    "repository": {
      "type": "git",
      "url": "https://github.com/gerosyab/exiv2-wasm.git"
    },
    "bugs": {
      "url": "https://github.com/gerosyab/exiv2-wasm/issues"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs",
        "default": "./dist/index.umd.js"
      },
      "./dist/*": "./dist/*"
    },
    "browser": "dist/index.umd.js",
    "unpkg": "dist/index.umd.js",
    "jsdelivr": "dist/index.umd.js",

    "sideEffects": false,
    "files": [
      "dist/*"
    ],
    "keywords": [
      "exiv2",
      "exif",
      "xmp",
      "iptc",
      "wasm",
      "webassembly",
      "embind"
    ]
}
  